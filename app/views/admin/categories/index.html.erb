<h2>分類列表 </h2>
<div>
    <%= link_to("New Category", new_admin_category_path, class: "btn btn-primary") %>
</div>
<table class="table table-bcategoryed">
  <thead>
    <tr>
      <th>id</th>
      <th>Name</th>

    </tr>
  </thead>
  <tbody>
    <% @categories.each do |category| %>
    <tr>
      <td> <%= category.id %> </td>
        <td><%= category.name %></td>
    </tr>

    <% end %>

  </tbody>
</table>
